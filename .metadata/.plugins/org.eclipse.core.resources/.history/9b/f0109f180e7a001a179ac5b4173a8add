public class Test {

	public static void main(String[] args) {
		int a[]= {100,14,46,47,94,52,86,36};int one = 0,two=0;
		System.out.println("given list");
		for(int i=0;i<a.length;i++) {
			System.out.print(+a[i]+"  ");
			
		}
		for(int i=0;i<a.length;i++) {
			if(a[i]>one) {
				one=a[i];
				two=one;
				
			}
			else if(a[i]>two) {
				two=a[i];
			}
		}
		System.out.println("\n largerst no::"+one);
		System.out.println("\n second largest no::"+two);
			}

}

